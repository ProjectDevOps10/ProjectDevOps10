{
  "name": "@iagent/workspace",
  "version": "1.0.0",
  "license": "MIT",
  "description": "iAgent AI Chat Application - Nx Workspace",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npx nx serve frontend",
    "dev:backend": "npx nx serve backend",
    "build": "npx nx run-many --target=build --projects=frontend,backend",
    "build:frontend": "npx nx build frontend",
    "build:backend": "npx nx build backend",
    "test": "npx nx run-many --target=test --projects=frontend,backend",
    "test:frontend": "npx nx test frontend",
    "test:backend": "npx nx test backend",
    "lint": "npx nx run-many --target=lint --projects=frontend,backend",
    "lint:frontend": "npx nx lint frontend",
    "lint:backend": "npx nx lint backend",
    "predeploy": "npm run build:frontend",
    "deploy": "gh-pages -d dist/apps/frontend",
    "deploy:gh-pages": "npm run predeploy && npm run deploy",
    "upgrade": "npx nx migrate latest && npm install && npx nx migrate --run-migrations",
    "clean": "npx nx reset && rm -rf node_modules && npm install",
    "graph": "npx nx graph",
    "affected": "npx nx affected"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.3",
    "@mui/x-date-pickers-pro": "^8.5.3",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@tailwindcss/vite": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.515.0",
    "mui-daterange-picker-plus": "^1.0.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "reflect-metadata": "^0.2.2",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.3.0",
    "aws-cdk-lib": "^2.124.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@nx/eslint": "^21.2.0",
    "@nx/eslint-plugin": "^21.2.0",
    "@nx/jest": "^21.2.0",
    "@nx/js": "^21.2.0",
    "@nx/nest": "^21.2.0",
    "@nx/node": "^21.2.0",
    "@nx/react": "^21.2.0",
    "@nx/vite": "^21.2.0",
    "@nx/web": "^21.2.0",
