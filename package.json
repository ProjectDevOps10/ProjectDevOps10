    "typescript": "~5.8.3",
    "typescript-eslint": "^8.19.0",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^6.0.1"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/iAgent.git"
  },
  "keywords": [
    "ai",
    "chat",
    "react",
    "nestjs",
    "nx",
    "typescript",
    "material-ui"
  ]
}
